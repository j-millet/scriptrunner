!lid_open => bash -c "~/.config/lidman/close-discharge.sh"
$:num_displays_plugged_in && last_display_was_connected => xrandr --output $:last_display_changed --auto && xrandr --output eDP --off
$:num_displays_plugged_in && !last_display_was_connected => xrandr --output $:last_display_changed --off && xrandr --output eDP --auto
